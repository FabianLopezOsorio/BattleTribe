n      <h2>Mensajes</h2>\r\n      <hr style=\"border-color: rgb(203, 204, 204)\" />\r\n      <transition-group\r\n        enter-active-class=\"animated bounceInDown\"\r\n        leave-active-class=\"animated fadeOutLeft\"\r\n        mode=\"out-in\"\r\n      >\r\n        <rectang-p v-for=\"mensaje in mensajesMostrar\" :key=\"mensaje\">\r\n          <p style=\"font-size:20px\">{{mensaje.Asunto}}</p>\r\n          <hr style=\"border-color: rgb(203, 204, 204); margin-top:0px\" />\r\n          <span class=\"badge pull-left\" style=\"font-size:10px;color:white\">Leidos: 42</span>\r\n          <span class=\"badge pull-left\" style=\"font-size:10px;color:white\">Respuestas: 3</span>\r\n          <router-link class=\"btn btn-default pull-right\" tag=\"div\" :to=\"{name:'mensaje'}\">\r\n            <a\r\n              @click=\"mensaje_ver({subject:mensaje.Asunto,content:mensaje.Contenido,id:mensaje._id})\"\r\n            >Reed more...</a>\r\n          </router-link>\r\n        </rectang-p>\r\n      </transition-group>\r\n      <div class=\"naveg\">\r\n        <button class=\"btn btn-primary navega\" @click=\"prev\" :disabled=\"pag<=0\">Previous</button>\r\n        <div class=\"navega\">{{pag}}</div>\r\n        <button\r\n          class=\"btn btn-primary navega\"\r\n          @click=\"next\"\r\n          :disabled=\"new_arr_m.length<=6+pag*6\"\r\n        >Next</button>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"container uno\">\r\n      <h3 style=\"text-align: center\">MÃ©dicos Activos</h3>\r\n      <hr style=\"border-color: rgb(203, 204, 204); margin-top:0px\" />\r\n      <p class=\"usermedic\">{{bienvenido}}</p>\r\n      <h3 style=\"text-align: center\">Pacientes Activos</h3>\r\n      <hr style=\"border-color: rgb(203, 204, 204); margin-top:0px\" />\r\n    </div>\r\n\r\n    <div class=\"form-group mensaj\">\r\n      <h3>Escriba su pregunta aqui</h3>\r\n      <input class=\"form-control\" type=\"text\" placeholder=\"Asunto\" v-model=\"asunto\" />\r\n      <textarea class=\"form-control\" placeholder=\"Mensaje\" cols=\"5\" rows=\"5\" v-model=\"texto\"></textarea>\r\n      <button\r\n        class=\"btn btn-primary\"\r\n        style=\"float: left;margin-top:4px;\"\r\n        @click=\"postear({msj:texto,titulo:asunto,id:new_arr_m.length})\"\r\n      >Post</button>\r\n    </div>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport rectangP from \"./rectang.vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      mensajesMostrar: [ ],\r\n      new_arr_m: [],\r\n      texto: \"\",\r\n      asunto: \"\",\r\n      pag: 0,\r\n      prueba: [],\r\n      error: \"\"\r\n    };\r\n  },\r\n  computed: {\r\n    bienvenido() {\r\n      return this.$store.state.nombre;\r\n    }\r\n  },\r\n  methods: {\r\n    postear: function(data) {\r\n     if (this.asunto !== \"\" && this.texto !== \"\") {\r\n        axios.post(\"http://localhost:3000/api/post/app/\", {\r\n        Asunto: data.titulo,\r\n        Contenido: data.msj\r\n      });\r\n        this.texto = \"\";\r\n        this.asunto = \"\";\r\n      }\r\n    },\r\n    next: function() {\r\n      if (this.new_arr_m.length > 6 * this.pag + 6) this.pag++;\r\n      for (let index = 0; index < 6; index++) {\r\n        if (index <= this.new_arr_m.length - 1 - 6 * this.pag) {\r\n          this.mensajesMostrar[index] = this.new_arr_m[\r\n            this.new_arr_m.length - 1 - index - 6 * this.pag\r\n          ];\r\n        } else {\r\n          this.mensajesMostrar.splice(index, 6);\r\n        }\r\n      }\r\n    },\r\n    prev: function() {\r\n      if (this.pag > 0) {\r\n        this.pag--;\r\n        for (let index = 0; index < 6; index++) {\r\n          if (index <= this.new_arr_m.length - 1 - 6 * this.pag) {\r\n            this.mensajesMostrar[index] = this.new_arr_m[\r\n              this.new_arr_m.length - 1 - index - 6 * this.pag\r\n            ];\r\n          }\r\n        }\r\n      }\r\n    },\r\n    mensaje_ver: function(b) {\r\n       axios.post(\"http://localhost:3000/api/post/actual/\", {\r\n        asunto: b.subject,\r\n        contenido: b.content,\r\n        id: b.id\r\n      });\r\n     // this.$store.dispatch(\"mensaje_ver\", b);\r\n    }\r\n  },\r\n  components: {\r\n    rectangP\r\n  },\r\n  created() {\r\n    axios.get(\"http://localhost:3000/api/post/\").then(docs=>{\r\n      this.new_arr_m= docs.data;\r\n      for (let index = 0; index < 6; index++) {\r\n      if (index < this.new_arr_m.length) {\r\n        this.mensajesMostrar.splice(\r\n          index,\r\n          1,\r\n          this.new_arr_m[this.new_arr_m.length - 1 - index]\r\n        );\r\n      }\r\n    }\r\n    });    \r\n  }\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n.bienvenido {\r\n  display: block;\r\n  margin: 5px;\r\n  padding: 5px;\r\n  background-color: rgba(80, 148, 238, 0.616);\r\n  box-shadow: 0px 0px 6px;\r\n  border-radius: 3px;\r\n}\r\n.proyectos {\r\n  padding: 5px;\r\n  height: 350px;\r\n  border-radius: 3px;\r\n}\r\n.container {\r\n  float: left;\r\n  border-radius: 3px;\r\n  background-color: rgba(134, 132, 132, 0.192);\r\n  margin: 0.5%;\r\n  height: 300px;\r\n  padding: 0;\r\n}\r\n.uno {\r\n  width: 23%;\r\n}\r\n.dos {\r\n  width: 75%;\r\n  padding: 20px;\r\n  height: 850px;\r\n}\r\n.naveg {\r\n  text-align: center;\r\n}\r\n.naveg .btn {\r\n  margin: 10px;\r\n}\r\n.navega {\r\n  width: 15%;\r\n  display: inline-block;\r\n  color: rgb(13, 13, 14);\r\n}\r\n.mensaj {\r\n  background-color: rgb(122, 122, 122);\r\n  display: inline-block;\r\n  width: 75.2%;\r\n  margin: 5px;\r\n  padding: 8px;\r\n  border-radius: 3px;\r\n  box-shadow: 0px 0px 8px rgb(122, 122, 122);\r\n}\r\n.mensaj h3 {\r\n  font-family: Cambria, Cochin, Georgia, Times, \"Times New Roman\", serif;\r\n  color: aliceblue;\r\n}\r\nspan {\r\n  font-size: 20px;\r\n  font-family: \"Segoe UI\", Tahoma, Geneva, Verdana, sans-serif;\r\n  color: black;\r\n  background-color: rgb(64, 70, 64);\r\n  border-radius: 3px;\r\n}\r\n.usermedic {\r\n  background-color: rgb(23, 128, 197);\r\n  display: inline-block;\r\n  margin: 3px;\r\n  padding: 3px;\r\n  border-radius: 2px;\r\n  color: rgb(254, 254, 255);\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}