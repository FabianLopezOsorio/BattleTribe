!==\"\")\r\n            {\r\n                axios.post(\"http://localhost:3000/api/post/resp/\", {\r\n                  contenido: data.contenido,\r\n                  from_id: data.id\r\n                 });\r\n                this.visualizar.unshift({contenido:this.answer,id:this.id});\r\n                this.answer=\"\";\r\n            }          \r\n        }\r\n    },\r\n    created(){\r\n        //this.id=this.$store.state.id;\r\n        axios.get(\"http://localhost:3000/api/post/actual/\").then(docs=>{\r\n            this.id=docs.data.id;\r\n            \r\n        });\r\n         axios.get(\"http://localhost:3000/api/post/resp/\").then(docs=>{\r\n            this.respuestas=docs.data;\r\n            \r\n        for (let index = 0; index < this.respuestas.length; index++) {\r\n            if(this.respuestas[index].from_id==this.id)\r\n            {\r\n               this.visualizar.push(this.respuestas[index]) ;\r\n               \r\n            }\r\n         }\r\n         });        \r\n    } \r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.user{\r\n    width: 18%;\r\n    border: 1px solid rgb(197, 197, 197);\r\n    float: inline-start;\r\n    border-radius: 3px;\r\n}\r\n.panel-default{\r\n    width: 82%;\r\n    height: 250px;\r\n}\r\n.user, .panel-default{\r\n    display: inline-block;\r\n}\r\n.container{\r\n    margin-top: 5px;\r\n}\r\n.panel-body{\r\n    overflow: hidden;\r\n}\r\n.pre-scrollable{\r\n    max-height: 180px;\r\n}\r\n.mensaj{\r\n  background-color: rgb(122, 122, 122);\r\n  display: inline-block;\r\n  width: 75.2%;\r\n  margin: 5px;\r\n  padding: 8px;\r\n  border-radius: 3px;\r\n  box-shadow: 0px 0px 8px rgb(122, 122, 122); \r\n}\r\n</style>"],"sourceRoot":"src/components"}]}