;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4FA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"escenario.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <div class=\"map\">  \r\n    <div class=\"perfil\">           \r\n      <h3>Tropas</h3>\r\n      <hr>\r\n      <label>Warriors: {{user.warriors.value}}  \r\n        Atk:{{user.warriors.atk+user.warriors.atk*user.atk_percent/100}} \r\n        Hp:{{user.warriors.hp+user.warriors.hp*user.hp_percent/100}}</label> \r\n      <label>Archers: {{user.archers.value}}  \r\n        Atk:{{user.archers.atk+user.archers.atk*user.atk_percent/100}} \r\n        Hp:{{user.archers.hp+user.archers.hp*user.hp_percent/100}}</label> \r\n      <hr>\r\n      <label>Continente: {{continente}}</label>\r\n      <label>Ir a:<input type=\"number\" v-model=\"continente\" max=\"10\"></label>\r\n      <hr>\r\n      <h4>Movimiento</h4>\r\n      <label>Hacia:<input type=\"number\" v-model=\"square\"></label>     \r\n      <label>Tropas a enviar:</label>\r\n      <label>Warriors:<input type=\"number\" v-model=\"war\"></label>     \r\n      <label>Archers:<input type=\"number\" v-model=\"arch\"></label> \r\n      <button @click=\"enviar\" class=\"btn btn-default\">Enviar</button>\r\n      <p>{{msg}}</p>\r\n    </div>\r\n\r\n    <div class=\"escenario\">\r\n       <div v-for=\"(square,index) in 200\" :key=\"square.id\" class=\"cuadrado\">\r\n         <img :src=\"getImgUrl(index)\" alt=\"...\" width=\"44px\" height=\"44px\" class=\"imagen\" @click=\"ir(index)\">\r\n       </div>\r\n    </div> \r\n   \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Axios from '@/service/api';\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      user:{},\r\n      continente:0,\r\n      square:0,\r\n      war:0,\r\n      arch:0,\r\n      msg:\"\"\r\n    };\r\n  },\r\n  methods: {\r\n     enviar: function () {\r\n       if (this.war>this.user.warriors.value || this.arch>this.user.archers.value) {\r\n         this.msg=\"No hay suficientes tropas\";\r\n       } else if(this.square>=2000){\r\n         this.msg=\"Ubicacion desconocida\";\r\n       }else{\r\n         this.msg=\"\";\r\n         Axios().post('actions/explorar/',{\r\n         user_id:this.$store.getters.user_id,\r\n         warriors:this.war,\r\n         archers:this.arch,\r\n         square:this.square\r\n\r\n       }).then(docs=>{\r\n         this.$store.dispatch(\"storage_user\",docs.data);\r\n         this.user=docs.data;                \r\n       })\r\n       }\r\n       \r\n     },\r\n     getImgUrl: function (params) {\r\n       var indice=params+this.continente*200;\r\n       var found = this.user.map_reveled.find(function(element) {\r\n          return element.cuadrado==indice;\r\n      });\r\n      if (found==undefined) {\r\n        return require('../assets/logo.png');\r\n      }else{\r\n        return require('../assets/'+found.path);\r\n      } \r\n     },\r\n     ir: function (params) {\r\n      var indice=params+this.continente*200;\r\n       this.square=indice;\r\n     }\r\n  },\r\n  \r\n  created() {\r\n    this.user=this.$store.getters.usuario;\r\n     \r\n   }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.map{\r\n  margin-top:5px;\r\n}\r\n.cuadrado{\r\n  height: 50px;\r\n  width: 50px;\r\n  background-color: rgb(136, 79, 15);\r\n  border: 3px solid rgb(0, 0, 0);\r\n  float:left;\r\n\r\n}\r\n.escenario{\r\n  overflow: auto;\r\n  width: 75%;\r\n}\r\n.perfil{\r\n  width: 25%;\r\n  border: 2px solid rgb(0, 0, 0);\r\n}\r\n.perfil,.escenario{\r\n  height: 500px;\r\n  background-color: rgb(146, 146, 146);\r\n  float:left;\r\n}\r\nhr{\r\n  margin-top: 0;\r\n}\r\nlabel{\r\n  width: 100%;\r\n}\r\n.imagen:hover{\r\nopacity: 0.5;\r\n}\r\n</style>"]}]}